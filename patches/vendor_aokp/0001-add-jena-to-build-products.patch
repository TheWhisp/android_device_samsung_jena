From 5798c9816997f400eed7e3712ee269f7767ee88a Mon Sep 17 00:00:00 2001
From: TheWhisp <daavis.90@gmail.com>
Date: Tue, 12 Mar 2013 20:48:55 +0200
Subject: [PATCH] add jena to build products

Change-Id: I86684099880cbe4c6270132ea94ff42b9a60fa79
---
 products/AndroidProducts.mk |    3 ++-
 products/jena.mk            |   18 ++++++++++++++++++
 vendorsetup.sh              |    1 +
 3 files changed, 21 insertions(+), 1 deletion(-)
 create mode 100755 products/jena.mk

diff --git a/products/AndroidProducts.mk b/products/AndroidProducts.mk
index 40d8a29..c99e016 100644
--- a/products/AndroidProducts.mk
+++ b/products/AndroidProducts.mk
@@ -26,4 +26,5 @@ PRODUCT_MAKEFILES := \
     $(LOCAL_DIR)/toro.mk \
     $(LOCAL_DIR)/toroplus.mk \
     $(LOCAL_DIR)/umtsspyder.mk \
-    $(LOCAL_DIR)/vs920.mk
+    $(LOCAL_DIR)/vs920.mk \
+    $(LOCAL_DIR)/jena.mk
diff --git a/products/jena.mk b/products/jena.mk
new file mode 100755
index 0000000..a7cf7a5
--- /dev/null
+++ b/products/jena.mk
@@ -0,0 +1,18 @@
+# Inherit AOSP device configuration for GT-S6500D.
+$(call inherit-product, device/samsung/jena/full_jena.mk)
+
+# Inherit AOKP common bits
+$(call inherit-product, vendor/aokp/configs/common.mk)
+
+# Inherit GSM common stuff
+$(call inherit-product, vendor/aokp/configs/gsm.mk)
+
+# Setup device specific product configuration.
+PRODUCT_NAME := aokp_jena
+PRODUCT_BRAND := samsung
+PRODUCT_DEVICE := jena
+PRODUCT_MODEL := GT-S6500D
+PRODUCT_MANUFACTURER := samsung
+
+PRODUCT_COPY_FILES += \
+    vendor/aokp/prebuilt/bootanimation/bootanimation_320_480.zip:system/media/bootanimation-alt.zip
diff --git a/vendorsetup.sh b/vendorsetup.sh
index 7a5da3a..40c062a 100644
--- a/vendorsetup.sh
+++ b/vendorsetup.sh
@@ -26,3 +26,4 @@ add_lunch_combo aokp_t0lte-userdebug
 add_lunch_combo aokp_targa-userdebug
 add_lunch_combo aokp_umtsspyder-userdebug
 add_lunch_combo aokp_vs920-userdebug
+add_lunch_combo aokp_jena-userdebug
\ No newline at end of file
-- 
