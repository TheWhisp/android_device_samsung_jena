import init.qcom.usb.rc

on early-init

	## Setup storage
	mkdir /storage 0775 system sdcard_r
    mkdir /storage/sdcard0 0775 system system
    mkdir /storage/sdcard1 0775 system system

    ## Backwards compability
    symlink /storage/sdcard0 /mnt/sdcard
    symlink /storage/sdcard0 /sdcard
    symlink /storage/sdcard1 /external_sd
    symlink /storage/sdcard1 /mnt/external_sd

    export EXTERNAL_STORAGE /storage/sdcard0
    export SECONDARY_STORAGE /storage/sdcard1

on fs
	mount_all /fstab.jena

on boot
	mount debugfs debugfs /sys/kernel/debug


	## Setup bluetooth
	mkdir /data/misc/bluetooth 0770 bluetooth bluetooth
    chown bluetooth bluetooth /sys/module/bluetooth_power/parameters/power
    chown bluetooth bluetooth /sys/class/rfkill/rfkill0/type
    chown bluetooth bluetooth /sys/class/rfkill/rfkill0/state
    chown bluetooth bluetooth /proc/bluetooth/sleep/proto
	chmod 6777 /system/bin/qcm_dut
    chown system system /sys/module/sco/parameters/disable_esco
    chmod 0660 /sys/module/bluetooth_power/parameters/power
    chmod 0660 /sys/class/rfkill/rfkill0/state
    chmod 0660 /proc/bluetooth/sleep/proto
    chown bluetooth bluetooth /sys/devices/platform/msm_serial_hs.0/clock
    chmod 0660 /sys/devices/platform/msm_serial_hs.0/clock
	chown bluetooth bluetooth /dev/ttyHS0
    chmod 0666 /dev/ttyHS0

	## Setup Wi-Fi
    mkdir /data/misc/wifi 0770 wifi wifi
    mkdir /data/misc/wifi/sockets 0770 wifi wifi
    mkdir /data/misc/wifi/wpa_supplicant 0775 wifi wifi
	mkdir /data/misc/wifi/hostapd 0770 wifi wifi
    mkdir /data/misc/dhcp 0770 dhcp dhcp
    chown dhcp dhcp /data/misc/dhcp

	chown radio radio /sys/devices/virtual/pdp/pdp/waketime

	chown system system /sys/class/leds/lcd-backlight/brightness
    chown system system /sys/class/leds/button-backlight/brightness
    chown system camera /sys/class/leds/flashlight/brightness
    chmod 0660 /sys/class/leds/lcd-backlight/brightness
    chmod 0660 /sys/class/leds/button-backlight/brightness

	# this value will be referd in msm7k liblight. we change permission of this file due to cts fail.
    chmod 0664 /sys/devices/virtual/sec/ts/brightness
    chown system system /sys/devices/virtual/sec/ts/brightness

	chown root system /dev/alps_io
    chmod 0660 /dev/alps_io

	chown system system /system/etc/sensors.dat
	chown system system /system/etc/calib.dat
	chown system system /system/etc/param.dat
	chmod 0664 /system/etc/sensors.dat
	chmod 0664 /system/etc/calib.dat
	chmod 0664 /system/etc/param.dat
    chown root radio /proc/cmdline
    chown system input /sys/class/sensors/accelerometer_sensor/calibration
    chown system system /sys/class/input/input2/enable
    chown radio system /sys/devices/virtual/sec/sec_touchkey/touchkey_raw_data
    chown radio system /sys/class/sec/sec_touchscreen/set_module_off
    chown radio system /sys/class/sec/sec_touchscreen/set_module_on
    chown radio system /sys/class/sec/sec_touchscreen/touchtype
    chown radio system /sys/class/sec/sec_touchscreen/set_all_reference
    chown radio system /sys/class/sec/sec_touchscreen/disp_all_refdata
    chown radio system /sys/class/sec/sec_touchscreen/set_all_inspection
    chown radio system /sys/class/sec/sec_touchscreen/disp_all_insdata
    chown radio system /sys/class/sec/sec_touchscreen/set_all_intensity
    chown radio system /sys/class/sec/sec_touchscreen/disp_all_intdata
    chown radio system /sys/class/sec/sec_touchscreen/firmware
    chown radio system /sys/class/sec/sec_touchscreen/get_threshold
    chown radio system /sys/class/sec/sec_touchscreen/get_touch_raw_data
	chmod 1664 sys/class/misc/zinitix_touch_misc
    chown radio fm_radio /sys/devices/platform/gpio-event/wakeup_keys

	# change permissions for battery
    chown sdcard_rw sdcard_rw /sys/class/power_supply/battery/call
    chown sdcard_rw sdcard_rw /sys/class/power_supply/battery/video
    chown sdcard_rw sdcard_rw /sys/class/power_supply/battery/music
    chown sdcard_rw sdcard_rw /sys/class/power_supply/battery/browser
    chown sdcard_rw sdcard_rw /sys/class/power_supply/battery/hotspot
    chown sdcard_rw sdcard_rw /sys/class/power_supply/battery/camera
    chown sdcard_rw sdcard_rw /sys/class/power_supply/battery/data_call
    chown system system /sys/class/power_supply/battery/gps
    chown sdcard_rw sdcard_rw /sys/class/power_supply/battery/batt_use

    chown system system /sys/devices/system/cpu/cpufreq/ondemand/boostfreq
    chmod 0664 /sys/devices/system/cpu/cpufreq/ondemand/boostfreq
    chown system system /sys/devices/system/cpu/cpufreq/ondemand/boostpulse
    chmod 0664 /sys/devices/system/cpu/cpufreq/ondemand/boostpulse
    chown system system /sys/devices/system/cpu/cpufreq/ondemand/boosttime
    chmod 0664 /sys/devices/system/cpu/cpufreq/ondemand/boosttime
    chown system system /sys/devices/system/cpu/cpufreq/ondemand/sampling_rate
    chmod 0664 /sys/devices/system/cpu/cpufreq/ondemand/sampling_rate
    chown system system /sys/devices/system/cpu/cpufreq/ondemand/io_is_busy
    chmod 0664 /sys/devices/system/cpu/cpufreq/ondemand/io_is_busy
    chown system system /sys/devices/system/cpu/cpufreq/ondemand/down_differential
    chmod 0664 /sys/devices/system/cpu/cpufreq/ondemand/down_differential
    chown system system /sys/devices/system/cpu/cpufreq/ondemand/powersave_bias
    chmod 0664 /sys/devices/system/cpu/cpufreq/ondemand/powersave_bias
    chown system system /sys/devices/system/cpu/cpufreq/ondemand/sampling_down_factor
    chmod 0664 /sys/devices/system/cpu/cpufreq/ondemand/sampling_down_factor
    chown system system /sys/devices/system/cpu/cpufreq/ondemand/up_threshold
    chmod 0664 /sys/devices/system/cpu/cpufreq/ondemand/up_threshold

    write /sys/devices/system/cpu/cpufreq/ondemand/up_threshold 90
    write /sys/devices/system/cpu/cpufreq/ondemand/sampling_rate 50000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 245760
    # Don't enable 1ghz OC by default
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 800000

    # Don't disable dmesg access for apps
    write /proc/sys/kernel/dmesg_restrict 0

    # USB and scard polling
    chmod 0444 /sys/devices/platform/msm_hsusb/gadget/usb_state
    chown system system /sys/devices/platform/msm_sdcc.1/polling
    chown system system /sys/devices/platform/msm_sdcc.2/polling
    chown system system /sys/devices/platform/msm_hsusb/gadget/wakeup

	# Permissions for CyanogenMod IO Scheduler settings
    chown system system /sys/block/mmcblk0/queue/scheduler
    chmod 0664 /sys/block/mmcblk0/queue/scheduler

	# KSM
    chown sytem system /sys/kernel/mm/ksm/run
    chmod 666 /sys/kernel/mm/ksm/run

	# For qmuxd socket
    mkdir /data/radio 0770 radio radio
    chown radio radio 0770 /data/radio

on post-fs-data
    ## Create log system
    mkdir /data/log 0775 system log

    mkdir /data/misc/radio 0775 radio system
    mkdir /data/radio 0770 radio radio

    setprop vold.post_fs_data_done 1

service rmt_storage /system/bin/rmt_storage /dev/block/mmcblk0p10 /dev/block/mmcblk0p11
    user root
    disabled

on property:ro.emmc=1
    start rmt_storage

service hci_qcomm_init /system/bin/log -t qcom-bluetooth -p i "hci_qcomm_init service requested"
    class main
    user bluetooth
    group bluetooth net_bt_admin
    disabled
    oneshot

service hciattach /system/bin/sh /system/etc/init.qcom.bt.sh
    class main
    user bluetooth
    group qcom_oncrpc bluetooth net_bt_admin system
    disabled
    oneshot

on property:bluetooth.hciattach=true
    start hciattach

on property:bluetooth.hciattach=false
    setprop bluetooth.status off

on property:init.svc.bluetoothd=running
    start abtfilt
    write /sys/class/bluetooth/hci0/idle_timeout 7000

on property:init.svc.bluetoothd=stopped
    stop abtfilt

service abtfilt /system/bin/abtfilt -a -d -z -n -q -v -s -w wlan0
    class main
    user root
    group bluetooth net_bt_admin inet net_admin net_raw
    disabled
    oneshot    

service wpa_supplicant /system/bin/logwrapper /system/bin/wpa_supplicant -Dwext -iwlan0 -W -c/data/misc/wifi/wpa_supplicant.conf
    class main
	group wifi inet
    socket wpa_wlan0 dgram 660 wifi wifi
    disabled	
    oneshot

service p2p_supplicant /system/bin/wpa_supplicant -Dwext -iwlan0 -c/data/misc/wifi/wpa_supplicant.conf
    class main
    socket wpa_wlan0 dgram 0660 wifi wifi
    disabled
    oneshot

service dhcpcd_bnep0 /system/bin/dhcpcd -ABDKL
    class main
    disabled
    oneshot

service iprenew_bnep0 /system/bin/dhcpcd -n
    class main
    disabled
    oneshot

service dhcpcd_wlan0 /system/bin/dhcpcd -aABDKL
    class main
    disabled
    oneshot

service iprenew_wlan0 /system/bin/dhcpcd -n
    class main
    disabled
    oneshot

service hostapd /system/bin/hostapd /data/misc/wifi/hostapd.conf
    socket hostapd_athap0 dgram 660 wifi wifi
    group wifi inet
    oneshot
    disabled

on property:init.svc.wpa_supplicant=stopped
    stop dhcpcd

service qmuxd /system/bin/qmuxd
    user radio
    group system radio inet

service bridgemgrd /system/bin/bridgemgrd
    user radio
    group radio
    disabled

service get_macaddrs /system/bin/get_macaddrs
    class main
    user root
    oneshot
    disabled

on property:init.svc.ril-daemon=running
    start get_macaddrs
